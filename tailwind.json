{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"hidden": {
		"style": {
			"display": "none"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"text-blue-800": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(30 64 175 / var(--tw-text-opacity))"
		}
	},
	"text-yellow-500": {
		"style": {
			"--tw-text-opacity": 1,
			"color": "rgb(234 179 8 / var(--tw-text-opacity))"
		}
	}
}